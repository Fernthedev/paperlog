{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Paper2 Scotland2",
    "id": "paper2_scotland2",
    "version": "4.0.0",
    "url": "https://github.com/Fernthedev/paperlog",
    "additionalData": {
      "overrideSoName": "libpaper2_scotland2.so",
      "cmake": false,
      "compileOptions": {
        "systemIncludes": ["shared/utfcpp/source"]
      }
    }
  },
  "workspace": {
    "scripts": {
      "build": ["cargo ndk --bindgen -o build -t arm64-v8a build --release"],
      "copy": ["pwsh ./scripts/copy.ps1"],
      "qmod": ["pwsh ./scripts/createqmod.ps1 $0"]
    },
    "qmodIncludeDirs": ["./build/arm64-v8a"],
    "qmodIncludeFiles": [],
    "qmodOutput": "paper2_scotland2.qmod"
  },
  "dependencies": [
    {
      "id": "scotland2",
      "versionRange": "^0.1.2",
      "additionalData": {
        "includeQmod": false,
        "private": true
      }
    },
    {
      "id": "fmt",
      "versionRange": "^11.0.2",
      "additionalData": {}
    }
  ]
}
