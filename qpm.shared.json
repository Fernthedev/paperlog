{
  "config": {
    "version": "0.1.0",
    "sharedDir": "shared",
    "dependenciesDir": "extern",
    "info": {
      "name": "Paper",
      "id": "paper",
      "version": "3.0.0",
      "url": "https://github.com/Fernthedev/paperlog",
      "additionalData": {
        "overrideSoName": "libpaperlog.so",
        "compileOptions": {
          "systemIncludes": [
            "shared/utfcpp/source"
          ]
        },
        "cmake": false
      }
    },
    "workspace": {
      "scripts": {
        "build": [
          "pwsh ./scripts/build.ps1"
        ],
        "copy": [
          "pwsh ./scripts/copy.ps1"
        ],
        "qmod": [
          "pwsh ./scripts/createqmod.ps1 $0"
        ],
        "test": [
          "pwsh ./scripts/build.ps1 --hostTarget",
          "./build_desktop/paperlog_test"
        ]
      }
    },
    "dependencies": [
      {
        "id": "fmt",
        "versionRange": "^10.0.0",
        "additionalData": {}
      }
    ]
  },
  "restoredDependencies": [
    {
      "dependency": {
        "id": "fmt",
        "versionRange": "=10.0.0",
        "additionalData": {
          "headersOnly": true,
          "branchName": "version/v10_0_0",
          "compileOptions": {
            "systemIncludes": [
              "fmt/include/"
            ],
            "cppFlags": [
              "-DFMT_HEADER_ONLY"
            ]
          }
        }
      },
      "version": "10.0.0"
    }
  ]
}